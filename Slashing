------------------------MISC -----------------------------------
-----------------------------------------------------------------

Misc:NewSlider("WalkSpeed", "increase speed", 150, 16, function(val)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = val
end)

Misc:NewSlider("JumpPower", "increase the jump", 150, 16, function(val)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = val
end)

Misc:NewToggle("InfJump", "Toggle infinite jump", function(bool)
    getgenv().infjump = bool
    if bool then
        infjump()
    end
end)

-----------------------Teleport ----------------------------------------

Tp:NewTextBox("Player Name: ","teleport a player",function (insert)
    local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
    local p2 = insert

    p1.CFrame = game.Players[insert].Character.HumanoidRootPart.CFrame
end)

Tp = Teleport:NewSection("World 1")

local selectWorld;
Tp:NewDropdown("Select Fase","Worlds", {"Medival","Candy","Lava"},function(value)
    selectWorld = value;
    print(value)
end)

Tp:NewButton("Teleport", "Button Teleport",function()
    function Teleport(world)
        local aa = world
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.LightingCHangeTriggers[world].CFrame
    end
    if selectWorld then
        Teleport(selectWorld)
    end
end)

Tp = Teleport:NewSection("World 2")

Tp:NewDropdown("Select Fase","Wolds", {"Atlantis", "Grave","Moon", "Desert", "Pirate"},function(value)
    selectWorld = value;
    print(value)
end)

