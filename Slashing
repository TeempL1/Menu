----------- by Temmpl

getgenv().Tap = false;
getgenv().egg = false;
getgenv().egg2 = false;
getgenv().infjump = false;

function Tap()
    game:GetService("RunService").Stepped:Connect(function()
        if getgenv().Tap then
            game:GetService("VirtualUser"):ClickButton1(Vector2.new(125,125))
        end
    end)
    plr = Players.LocalPlayer
    mouse = plr:GetMouse()
    mouse.KeyDow:Connect(function(key)
        if key == "t" then
            getgenv().Tap = not getgenv().Tap
        end
   end)
end

function Egg(selectedEgg)
    while wait(1) do
        if not getgenv().egg then
            break
        end
        local A_1 = selectedEgg
        local A_2 = "Coins"
        local Event = game:GetService("ReplicatedStorage").Remotes.Pets.Eggs.HatchEgg
        Event:FireServer(A_1, A_2)
    end
end

function Egg2(selectedEgg)
    while wait(1) do
        if not getgenv().egg2 then
            break
        end
        local A_1 = selectedEgg
        local A_2 = "Diamonds"
        local Event = game:GetService("ReplicatedStorage").Remotes.Pets.Eggs.HatchEgg
        Event:FireServer(A_1, A_2)
    end
end

function infjump() 
    game:GetService("UserInputService").JumpRequest:connect(function()
        if getgenv().infjump then
            game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
        end
    end)
end


local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local window = lib.CreateLib("Teemp ModMenu", "BloodTheme")
local maintab = window:NewTab("Farm")
local second = window:NewTab("Misc")
local Teleport = window:NewTab("Teleports")

local Main = maintab:NewSection("Farming")
local Misc = second:NewSection("Misc")
local Tp = Teleport:NewSection("Teleport Player") 

---------------------Farming ----------------------------------
Main:NewToggle("AutoTap","Toggle Tap", function (bool)
    getgenv().Tap = bool
    if bool then 
        Tap();
    end
end)

Main:NewButton("Super Rebirth", "Button Rebirth",function ()
    local lp = game.Players.LocalPlayer.Character.HumanoidRootPart
 
    lp.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    lp.CFrame = CFrame.new(150.9437255859375, 4.1143693923950195, 243.56137084960938)
end)

Main:NewButton("Sell", "Button Sell",function ()
    local lp = game.Players.LocalPlayer.Character.HumanoidRootPart
    local pos = lp.CFrame
 
    lp.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    lp.CFrame = CFrame.new(222.30462646484375, 4.114368438720703, 384.5313720703125)
    wait(0.3)
 
     lp.CFrame = pos
end)

Main:NewButton("Swords", "Buy Sword", function ()
    local lpr = game.Players.LocalPlayer.Character.HumanoidRootPart

    lpr.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    lpr.CFrame = CFrame.new(222.86009216308594, 4.114368438720703, 399.48681640625)

end)
--------------------------------WORLD 1 EGGS -------------------------------------------------
Main = maintab:NewSection("World 1 Eggs")

local selectedword1Egg;
Main:NewDropdown("Select Eggs","you must be close to the egg to work", {"BasicEgg","WizardEgg","FrozenEgg","CandyEgg", "MagmaEgg"}, function(value)
    selectedword1Egg = value;
    print(value)
end)
Main:NewToggle("Buy Eggs","you must be close to the egg to work",function(bool)
    getgenv().egg = bool
    if bool and selectedword1Egg  then
        Egg(selectedword1Egg);
    end
end)
