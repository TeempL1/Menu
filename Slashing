    function Teleport(world)
        local aa = world
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.WORLDS.World3.Gameplay.RegionBariers[world].CFrame
    end
    if selectWorld then
        Teleport(selectWorld)
    end
end)
